            ],
            "settings": {
                "foreground": "#8CDAFF"
            }
        },
        {
            "name": "Attribute Selector String Value", // temporary hack
            "scope": [
                "meta.attribute-selector"
            ],
            "settings": {
                "foreground": "#FBDFB5"
            }
        },
        {
            "name": "At Rule (`@mixin`, `@import`, etc.)",
